1.cpp


class Solution {
public:
    bool isAnagram(string s, string t) {
        if(s.length() != t.length()){
            return false;
        }
        unordered_map<char, int>count;
        for(char ch:s){
        count[ch] =count[ch]+1;
    }
    for(char ch:t){
        if(count.find(ch) == count.end() || count[ch] == 0){
            return false;
        }
        count[ch] = count[ch]-1;
    }
    return true;
    }
};
