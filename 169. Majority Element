1.cpp:

class Solution {
public:
    int majorityElement(vector<int>& nums) {
        unordered_map<int,int>v;
        int n=nums.size();
        int a=n/2;
        for(int i : nums){
            v[i]++;
            if(v[i]>a){
                return i;
            }
        }
        return -1;
    }      
};
