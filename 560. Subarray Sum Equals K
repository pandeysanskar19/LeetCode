c++


class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        unordered_map<int,int>sub;
        sub[0] =1;
        int sum =0,count=0;
    for(int n:nums){
        sum +=n;
        if(sub.find(sum - k) != sub.end()){
            count += sub[sum -k];
        }
        sub[sum]++;
        }
        return count;
    }
};
