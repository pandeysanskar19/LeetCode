1.BRUTE FORCE(CPP)
class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        int n = nums.size();
        for(int i=1;i<n;i++){
            if(nums[i] == nums[i-1])
            return true;
        }
        return false;
    }
};



2.Hash map(cpp)
class Solution {
public:
    bool containsDuplicate(vector<int>& nums) {
      unordered_map<int , int> seen;
      for(int num:nums){
        if(seen[num]>=1)
        return true;
        seen[num]++;
      }
      return false;
    }
};
